{
    "name": "@bspk/ui",
    "version": "1.1.28",
    "license": "CC-BY-4.0",
    "type": "module",
    "files": [
        "dist",
        "src",
        "README.md",
        "LICENSE",
        "meta.ts",
        "meta-types.ts"
    ],
    "exports": {
        "./*": "./dist/*.js",
        ".": "./dist/index.js"
    },
    "scripts": {
        "sass": "sass --style=compressed --no-source-map src:dist",
        "build": "tsx build.ts",
        "lint:css": "npx stylelint '**/*.scss'",
        "lint": "eslint && tsc --noEmit && stylelint '**/*.scss'",
        "lint:fix": "eslint --fix",
        "semantic-release": "semantic-release",
        "test": "jest",
        "prepare": "husky",
        "pre:commit": "tsx .scripts/pre-commit.ts",
        "tsc": "tsc",
        "meta": "tsx meta.ts",
        "pretty": "npx prettier --write src/"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=20"
    },
    "dependencies": {
        "@bspk/styles": "^1.0.0",
        "@floating-ui/dom": "^1.6.13",
        "focus-trap-react": "^11.0.1"
    },
    "peerDependencies": {
        "@bspk/icons": "^1.0.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@cspell/eslint-plugin": "^8.17.5",
        "@eslint/css": "^0.8.1",
        "@eslint/js": "^9.14.0",
        "@playwright/test": "^1.52.0",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.2",
        "@semantic-release/npm": "^12.0.1",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/eslint-plugin-jsx-a11y": "^6.10.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.2",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.5",
        "eslint": "9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "globals": "^15.12.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "prettier-plugin-jsdoc": "^1.3.0",
        "sass": "^1.89.0",
        "semantic-release": "^24.2.0",
        "stylelint": "^16.19.1",
        "stylelint-config-standard": "^38.0.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "ts-jest": "^29.2.5",
        "tslib": "^2.8.1",
        "tsx": "^4.19.4",
        "typescript": "5.6.3",
        "typescript-eslint": "8.13.0",
        "typescript-json-schema": "^0.65.1"
    }
}
