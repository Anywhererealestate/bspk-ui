{
    "name": "@bspk/ui",
    "version": "1.1.1",
    "main": "index.js",
    "license": "CC-BY-4.0",
    "type": "module",
    "scripts": {
        "tasks": "tsx .scripts/tasks/lint-components.ts",
        "sass": "sass --style=compressed --no-source-map src:dist",
        "build": "tsx .scripts/build.ts",
        "build:css": "tsx .scripts/build-css.ts",
        "meta": "tsx .scripts/build-meta.ts",
        "lint:css": "npx stylelint '**/*.scss' ",
        "lint": "eslint && tsc --noEmit",
        "lint:fix": "eslint --fix",
        "rollup": "rollup -c",
        "semantic-release": "semantic-release",
        "test": "jest",
        "setup:dist": "tsx .scripts/setup-dist.ts",
        "pack": "rm -rf ./dist && npm run build && npm run setup:dist && cd dist && npm pack && cd ..",
        "prepare": "husky",
        "update:css": "npm un @bspk/styles && npm i @bspk/styles@latest && npm run build:css && npm run lint:style",
        "preview": "npm run pack && cd dist && tar -xvf ./bspk-ui-*.tgz && cd package && npm link",
        "watch:dev": "tsx .scripts/watch-dev.ts",
        "pre:commit": "tsx .scripts/pre-commit.ts"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=20"
    },
    "dependencies": {
        "@bspk/icons": "^1.0.3",
        "@bspk/styles": "^1.0.0",
        "@floating-ui/dom": "^1.6.13",
        "focus-trap-react": "^11.0.1",
        "style-inject": "^0.3.0"
    },
    "peerDependencies": {
        "react": "^18.3.1"
    },
    "devDependencies": {
        "@cspell/eslint-plugin": "^8.17.5",
        "@eslint/css": "^0.8.1",
        "@eslint/js": "^9.14.0",
        "@rollup/plugin-commonjs": "^28.0.3",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-typescript": "^12.1.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.2",
        "@semantic-release/npm": "^12.0.1",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/eslint-plugin-jsx-a11y": "^6.10.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.2",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.5",
        "eslint": "9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "globals": "^15.12.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "postcss": "^8.5.3",
        "prettier-plugin-jsdoc": "^1.3.0",
        "react-dom": "^18.3.1",
        "rollup": "^2.79.2",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-sass": "^1.15.2",
        "semantic-release": "^24.2.0",
        "stylelint": "^16.19.1",
        "stylelint-config-standard": "^38.0.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "tsx": "^4.19.4",
        "typescript": "5.6.3",
        "typescript-eslint": "8.13.0",
        "typescript-json-schema": "^0.65.1"
    }
}
